{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@digitalasset/*": ["*", "bazel-bin/*", "bazel-genfiles/*"]
    },
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "exlude": ["bazel-*", "node_modules"]
}
